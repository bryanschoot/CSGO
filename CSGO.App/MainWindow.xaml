<Window x:Class="CSGO.App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSGO.App"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="inject_btn" Content="Inject" HorizontalAlignment="Left" Height="25" Margin="10,10,0,0" VerticalAlignment="Top" Width="125" Click="Inject_btn_Click"/>
        <Label x:Name="status_lbl" Content="Status" HorizontalAlignment="Left" Margin="140,10,0,0" VerticalAlignment="Top" Height="25" Width="115"/>
        <CheckBox x:Name="wallhack_cb" Content="Wallhack" Checked="Wallhack_cb_Checked" Unchecked="Wallhack_cb_UnChecked" Margin="10,40,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <CheckBox x:Name="recoil_cb" Content="Recoil" Checked="Recoil_cb_Checked" Unchecked="Recoil_cb_UnChecked" Margin="10,60,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
</Window>
